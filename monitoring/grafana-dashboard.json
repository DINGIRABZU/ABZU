{
  "title": "ABZU FastAPI Metrics",
  "panels": [
    {
      "type": "graph",
      "title": "Request Rate",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m]))",
          "legendFormat": "requests"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Request Duration",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ]
    },
    {
      "type": "graph",
      "title": "CPU Usage",
      "targets": [
        {
          "expr": "100 - (avg(irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
          "legendFormat": "cpu"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Memory Usage",
      "targets": [
        {
          "expr": "(1 - node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes) * 100",
          "legendFormat": "memory"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Disk Usage",
      "targets": [
        {
          "expr": "(1 - node_filesystem_avail_bytes{mountpoint=\"/\"} / node_filesystem_size_bytes{mountpoint=\"/\"}) * 100",
          "legendFormat": "disk"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Network I/O",
      "targets": [
        {
          "expr": "rate(node_network_receive_bytes_total[5m])",
          "legendFormat": "rx"
        },
        {
          "expr": "rate(node_network_transmit_bytes_total[5m])",
          "legendFormat": "tx"
        }
      ]
    },
    {
      "type": "graph",
      "title": "GPU Utilization",
      "targets": [
        {
          "expr": "avg(DCGM_FI_DEV_GPU_UTIL)",
          "legendFormat": "gpu"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Service CPU Usage",
      "targets": [
        {
          "expr": "service_cpu_usage_percent",
          "legendFormat": "{{service}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Service Memory Usage",
      "targets": [
        {
          "expr": "service_memory_usage_bytes",
          "legendFormat": "{{service}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Service GPU Memory",
      "targets": [
        {
          "expr": "service_gpu_memory_usage_bytes",
          "legendFormat": "{{service}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Service Request Latency",
      "targets": [
        {
          "expr": "service_request_latency_seconds",
          "legendFormat": "{{service}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Alpha Gate Phase Duration",
      "targets": [
        {
          "expr": "alpha_gate_phase_duration_seconds",
          "legendFormat": "{{phase}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Alpha Gate Phase Success",
      "targets": [
        {
          "expr": "alpha_gate_phase_success",
          "legendFormat": "{{phase}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Alpha Gate Coverage",
      "targets": [
        {
          "expr": "alpha_gate_coverage_percent",
          "legendFormat": "coverage"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Crown Replay Divergences",
      "targets": [
        {
          "expr": "crown_replay_divergences_total"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Crown Replay Duration",
      "targets": [
        {
          "expr": "crown_replay_duration_seconds"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "type": "stat",
      "title": "Crown Replay Scenarios",
      "targets": [
        {
          "expr": "crown_replay_scenarios_total"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Boot First Attempt Successes",
      "targets": [
        {
          "expr": "razar_boot_first_attempt_success_total"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Boot Retry Attempts",
      "targets": [
        {
          "expr": "razar_boot_retry_total"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Boot Total Time",
      "targets": [
        {
          "expr": "razar_boot_total_time_seconds"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "type": "graph",
      "title": "Beta External Feedback Latency (p95)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(beta_feedback_latency_ms_bucket[5m])) by (le, channel))",
          "legendFormat": "{{channel}}"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Beta Feedback Error Budget",
      "targets": [
        {
          "expr": "avg by (channel) (beta_feedback_error_budget_ratio)",
          "legendFormat": "{{channel}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        }
      }
    },
    {
      "type": "graph",
      "title": "Beta Feedback Satisfaction",
      "targets": [
        {
          "expr": "avg by (channel, signal) (beta_feedback_satisfaction_score)",
          "legendFormat": "{{channel}} {{signal}}"
        }
      ]
    }
  ]
}
