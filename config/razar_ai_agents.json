{
  "active": "demo_agent",
  "agents": [
    {
      "name": "demo_agent",
      "endpoint": "https://example.com/api",
      "auth": {
        "type": "bearer",
        "token": "DEMO_TOKEN"
      }
    },
    {
      "name": "backup_agent",
      "endpoint": "https://backup.example.com/api",
      "auth": {
        "type": "api_key",
        "key": "BACKUP_KEY"
      }
    },
    {
      "name": "kimi2",
      "endpoint": "${KIMI2_ENDPOINT}",
      "auth": {
        "type": "bearer",
        "token": "${KIMI2_API_KEY}"
      }
    },
    {
      "name": "airstar",
      "endpoint": "${AIRSTAR_ENDPOINT}",
      "auth": {
        "type": "bearer",
        "token": "${AIRSTAR_API_KEY}"
      }
    },
    {
      "name": "rstar",
      "endpoint": "${RSTAR_ENDPOINT}",
      "auth": {
        "type": "bearer",
        "token": "${RSTAR_API_KEY}"
      }
    }
  ]
}
